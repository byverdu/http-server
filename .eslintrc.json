{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": "standard",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
    "semi": "off",
    "space-before-function-paren": "off",
    "comma-dangle":"off"
  }
}
